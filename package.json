    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mocha-clean": "^1.0.0",
    "ts-node": "^0.9.3",
    "typescript": "^2.0.0",
    "@types/vue": "^1.0.25-alpha",
    "@types/vue-router": "^0.7.25-alpha",
    "@types/chai": "^3.4.27-alpha",
    "@types/mocha": "^2.2.26-alpha"
  },
  "dependencies": {
    "vue": "^1.0.26"
  },
  "peerDependencies": {
    "vue": "^1.0.26"
  }
