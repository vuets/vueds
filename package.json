{
  "name": "vueds",
  "version": "0.1.0",
  "description": "vue and protostuff-ds",
  "private": true,
  "author": "dyu",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "test": "mocha",
    "dev": "mocha -w",
